{
  "name": "imx-svelte-frontend",
  "version": "0.0.1",
  "license": "AGPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "git://iyi.cz/imx/svelte_frontend.git"
  },
  "browser": {
    "custom-fetch.js": "./custom-fetch-browser.js"
  },
  "@comments scripts": {
    "dev": "develop with blazing fast rebuilds",
    "dev:features": "develop with features like SSR and serviceworker enabled",
    "build": "run build scripts below",
    "build:app": "build single page application (SPA)",
    "build:static": "Generate static pages",
    "serve": "serve content in 'dist' folder",
    "rollup": "run the rollup bundler",
    "nollup": "run the nollup no-bundler",
    "routify": "run routify"
  },
  "scripts": {
    "dev": "run-p routify nollup",
    "dev:ssr": "run-p routify rollup",
    "build": "run-s build:*",
    "build:app": "routify -b && rollup -c",
    "build:static": "spank",
    "serve": "spassr --ssr",
    "rollup": "rollup -cw",
    "nollup": "nollup -c",
    "routify": "routify"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@roxi/routify": "^2.18.5",
    "@types/marked": "^4.0.3",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.8.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.15.0",
    "fs-extra": "^10.1.0",
    "marked": "^4.0.15",
    "mdsvex": "^0.10.5",
    "nollup": "^0.19.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.13",
    "postcss-import": "^14.1.0",
    "rollup": "^2.72.0",
    "rollup-plugin-hot": "^0.1.1",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-svelte-hot": "^0.14.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-workbox": "^6.2.0",
    "spank": "^1.9.0",
    "spassr": "^2.6.0",
    "svelte": "^3.48.0",
    "svelte-i18n": "^3.4.0",
    "svelte-infinite-loading": "^1.3.8",
    "svelte-preprocess": "^4.10.6",
    "svelte-tiny-virtual-list": "^2.0.3",
    "sveltestrap": "^5.9.0",
    "tossr": "^1.4.2"
  },
  "routify": {
    "extensions": "svelte,html,svx,md"
  },
  "spassr": {
    "assetsDir": [
      "dist",
      "assets"
    ],
    "script": "dist/build/main.js",
    "ssrOptions": {
      "inlineDynamicImports": true
    }
  },
  "spank": {
    "blacklist": [
      "/example/modal/basic/4"
    ]
  },
  "dependencies": {
    "cl-editor": "^2.3.0",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte-notifications": "^0.9.93",
    "svelte-prismjs": "^1.0.2",
    "xterm": "^4.18.0",
    "xterm-addon-fit": "^0.5.0",
    "xterm-addon-web-links": "^0.5.1"
  }
}
